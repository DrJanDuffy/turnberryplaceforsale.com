{
  "name": "turnberryplaceforsale",
  "version": "1.5.0",
  "description": "Luxury real estate site built with Next.js, Drupal, Cursor AI, Vercel, and GitHub",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "preview": "next build && next start -p 3001",
    "lint": "next lint",
    "asset:audit": "node scripts/asset-audit.js",
    "asset:optimize": "node scripts/optimize-public-images.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test 'yarn preview' http://localhost:3001 cy:open",
    "test:e2e:ci": "start-server-and-test 'yarn preview' http://localhost:3001 cy:run",
    "vbuild": "npx vercel build",
    "check": "npm run lint && npx vercel build",
    "deploy": "git add . && git commit -m 'update' && git push"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-google-maps/api": "^2.20.8",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "@tanstack/react-query": "^4.36.1",
    "classnames": "^2.5.1",
    "drupal-jsonapi-params": "^2.3.1",
    "html-react-parser": "^4.2.10",
    "lucide-react": "^0.562.0",
    "next": "^14.2.2",
    "next-drupal": "^2.0.0-beta.0",
    "nprogress": "^0.2.0",
    "photoswipe": "^5.4.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@types/node": "25.0.6",
    "@types/react": "19.2.8",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.2",
    "postcss": "^8.4.38",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  }
}
